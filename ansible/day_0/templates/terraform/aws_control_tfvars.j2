network_state_file = "../Provision_VPC/terraform.tfstate.d/aws_network/terraform.tfstate"
vbond_ami = "{{ vbond_image_id }}"
vmanage_ami = "{{ vmanage_image_id }}"
vmanage_count = "1"
vsmart_ami = "{{ vsmart_image_id }}"
vsmart_count = "1"
vbond_instances_type = "{{ vbond_instance_type }}"
vmanage_instances_type = "{{ vmanage_instance_type }}"
vsmart_instances_type = "{{ vsmart_instance_type }}"
ssh_pubkey = "{{ ssh_pubkey }}"
sdwan_org = "{{ vmanage_org }}"
{% if aws_route53_zone is defined and aws_route53_zone != "" %}
route53_zone = "{{ aws_route53_zone }}"
{% endif %}
vmanage_day0 = <<EOF
{{ hostvars[groups['vmanage_hosts'][0]]['day0_config'] }}
EOF
vbond_day0 = <<EOF
{{ hostvars[groups['vbond_hosts'][0]]['day0_config'] }}
EOF
vsmart_day0 = <<EOF
{{ hostvars[groups['vsmart_hosts'][0]]['day0_config'] }}
EOF
